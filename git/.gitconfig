[user]
  name = rotsix
  email = rotsix@openmailbox.org
	
[alias]
  ci = commit
  co = checkout
  st = status
  br = branch

[push]
  default = simple

[color]
  ui = auto
  diff = auto
  branch = auto
  status = auto

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow
  frag = magenta
  old = red
  new = green
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
